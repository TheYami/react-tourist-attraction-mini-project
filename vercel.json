{
    "version": 2,
    "builds": [
      {
        "src": "client/package.json",
        "use": "@vercel/next" 
      },
      {
        "src": "server/**/*.js",
        "use": "@vercel/node" 
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/client/$1"
      },
      {
        "src": "/api/(.*)",
        "dest": "/server/$1.js"
      }
    ]
  }
  